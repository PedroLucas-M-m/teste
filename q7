mport requests
import json

url = "https://novorevan.brisanet.net.br/telefones.txt"
 
resposta = requests.get(url)

numerosStr = resposta.text

numeros = []

def inserirNumeros(data):
    n = 7
    for index in range(0, len(data), n):
        numeros.append(data[index : index + n].rstrip("\n"))

def validaNumeros(numeros):
    for i in numeros:
        validaNumero(i)
            

def validaNumero(numero):
    for i in numero:
        if i == i + 1:
            numeros.remove(numero)
        elif sum(i) % 2 != 0:
            numeros.remove(numero)
        elif i[0] == i[5]:
            numeros.remove(numero)         

inserirNumeros(numerosStr)
validaNumeros(numeros)

print(numeros)


